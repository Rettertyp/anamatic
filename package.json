{
    "name": "anamatic",
    "version": "0.0.0",
    "packageManager": "yarn@4.12.0",
    "scripts": {
        "ng": "ng",
        "start": "concurrently --names \"Frontend,Backend\" -c \"bgBlue.bold,bgMagenta.bold\" \"yarn start:frontend\" \"yarn start:backend\" --prefix \"[{time} - {name}]\" --timestamp-format \"HH:mm:ss.SSS\"",
        "start:frontend": "nx serve anamatic",
        "start:backend": "nx serve api",
        "build:frontend": "nx build anamatic",
        "build:backend": "nx build api",
        "server:backend": "node dist/apps/api/main.js",
        "watch": "nx build --watch --configuration development",
        "test": "nx run-many -t test"
    },
    "private": true,
